{
  "author": "Than Sar",
  "description": "Experimental version of AniMouto with bug fixes",
  "name": "AniMouto Experimental",
  "version": "0.0.6",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "storage",
    "identity",
    "alarms"
  ],
  "optional_permissions": [
    "notifications"
  ],
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
    },
    "default_popup": "src/entries/popup/index.html"
  },
  "background": {
    "service_worker": "serviceWorker.js",
    "type": "module"
  },
  "manifest_version": 3,
  "key": ""
}